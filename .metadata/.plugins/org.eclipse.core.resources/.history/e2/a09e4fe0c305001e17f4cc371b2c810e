


//読み込み時に自動起動
$(function(){
	
	let searchFlg = $("#searchFlg").val();
	alert(searchFlg);
	
	if(searchFlg == 1) searchEdmng();
	
});


function searchEdmng(){
	
	$("#searchFlg").val(1);
	let data = $("#searchForm").serialize();
	
	$.post(
		"getPages4Edmng",
		data,
		function(rtn){
			alert(rtn);
		}
	)
	
}