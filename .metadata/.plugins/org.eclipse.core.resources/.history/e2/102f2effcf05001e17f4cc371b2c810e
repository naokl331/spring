import org.springframework.web.bind.annotation.ModelAttribute;

import com.example.katodo.data.EdmngData;

@Controller
public class EdmngController {

	//------------------------------------------------
	//			  受注管理画面表示
	//------------------------------------------------
	@GetMapping("/edmng")
	public String index(@ModelAttribute("ed") EdmngData ed, Model model) {
		
		return "edmng";
	}
	
}
